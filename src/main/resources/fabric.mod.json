{
  "schemaVersion": 1,
  "id": "vigorem",
  "version": "${version}",
  "name": "Vigorem",
  "description": "Bring life to the minecraft player's animations (and more!)",
  "authors": [
    "Arathain"
  ],
  "contact": {
    "issues": "https://github.com/Arathain/Vigorem/issues",
    "sources": "https://github.com/Arathain/Vigorem"
  },
  "license": "ARR",
  "icon": "assets/vigorem/icon.png",
