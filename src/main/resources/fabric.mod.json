  "entrypoints": {
    "main": [
      "arathain.vigorem.Vigorem"
    ],
    "client": [
      "arathain.vigorem.VigoremClient"
    ],
    "preLaunch": [
      "arathain.vigorem.VigoremPreLaunch"
    ],
    "cardinal-components-entity": [
      "arathain.vigorem.VigoremComponents"
    ]
  },
  "mixins": [
    "vigorem.mixins.json"
  ],
  "depends": {
